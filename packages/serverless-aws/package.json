{
  "name": "@celerity-sdk/serverless-aws",
  "version": "0.3.0",
  "description": "AWS Lambda adapter for the Celerity Node SDK â€” converts API Gateway events to SDK types",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/newstack-cloud/celerity-node-sdk.git",
    "directory": "packages/serverless-aws"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./handler": {
      "import": {
        "types": "./dist/entry.d.ts",
        "default": "./dist/entry.js"
      },
      "require": {
        "types": "./dist/entry.d.cts",
        "default": "./dist/entry.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run"
  },
  "dependencies": {
    "@celerity-sdk/core": "workspace:^",
    "@celerity-sdk/types": "workspace:^",
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.160"
  },
  "publishConfig": {
    "access": "public"
  }
}
